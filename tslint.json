{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "member-ordering": false,
        "max-line-length": false,
        "no-bitwise": false,
        "no-namespace": false,
        "no-empty-interface": false,
        "no-empty": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}